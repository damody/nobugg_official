<!--scrolling nav fixed-->
<script>
	jQuery("document").ready(function($){
		var nav = $('header');
		
		$(window).scroll(function () {
			if ($(this).scrollTop() > 50) {
				nav.addClass("nav-sticky");
			} else {
				nav.removeClass("nav-sticky");
			}
		});
	 
	});
</script>
